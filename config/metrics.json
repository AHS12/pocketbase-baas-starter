{
  "provider": "prometheus",
  "enabled": true,
  "namespace": "ims_pocketbase",
  "labels": {
    "service": "pocketbase",
    "environment": "development"
  },
  "prometheus": {
    "metrics_path": "/metrics",
    "buckets": [0.001, 0.005, 0.01, 0.025, 0.05, 0.1, 0.25, 0.5, 1.0, 2.5, 5.0, 10.0]
  },
  "opentelemetry": {
    "endpoint": "http://localhost:4317",
    "headers": {},
    "insecure": true,
    "export_interval": "30s"
  }
}